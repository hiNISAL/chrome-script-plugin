{
    "name": "script",
    "manifest_version": 3,
    "version": "0.1",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_title": "Settings",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://www.google.com.hk/"],
            "css": ["float-window.css"],
            "js": ["recoder-popper.js"]
        }
    ]
}